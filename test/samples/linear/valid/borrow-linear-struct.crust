
linear struct lin_point {
    int x;
    int y;
};

unrestricted struct point {
    int x;
    int y;
};

int sum_points(ref struct lin_point a, ref struct point b) {

    return (a->x + b->x + b->x) * (b->y + b->y + a->y);
}

int main() {
    struct lin_point a;
    struct point b;
 
    
    return sum_points(&a, &b);
} 
